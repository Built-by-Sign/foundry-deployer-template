# Required for deployments
PRIVATE_KEY=                    # Deployer wallet private key (hex, with or without 0x prefix)
MAINNET_RPC_URL=                # RPC endpoint for mainnet (e.g., https://eth-mainnet.g.alchemy.com/v2/YOUR_KEY)
SEPOLIA_RPC_URL=                # RPC endpoint for Sepolia testnet

# Optional: foundry-deployer config
PROD_OWNER=                     # Address that receives ownership on mainnet deployments (multisig recommended)
MAINNET_CHAIN_IDS=1,56,137,8453 # Chain IDs considered "mainnet" â€” triggers ownership transfer to PROD_OWNER
ALLOWED_DEPLOYMENT_SENDER=      # Restrict who can run deployment scripts (leave empty to allow any sender)
FORCE_DEPLOY=false              # Set to "true" to redeploy even if an artifact already exists for this version
